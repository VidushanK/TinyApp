
<header>
  <% if (username) { %>
    <h1>Current user: <%=username.email%> </h1>

    <form action='/logout' method='POST'>
      <input type='submit' value="Logout">
    </form>
  <% } else { %>
    <a href="/login"> <button> login </button></a>
    <a href="/register"> <button> register </button></a>
  <% } %>
</header>
