<!DOCTYPE html>
<html lang="en">
  <head>
    <title>TinyApp</title>
  </head>
  <body>
    <% if (username !== undefined) { %>
    <form action="/logout" method="POST">
      <input type="submit" value="logout">
    </form>
     <% } else{ %>
    <form action="/login" method="POST">
      <input id="username" type="text" name="username" placeholder="username" style="width: 150px">
      <input type="submit" value="Submit">
    </form>
    <%}%>

  </body>
</html>